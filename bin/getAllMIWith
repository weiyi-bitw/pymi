#!/usr/bin/env python

import sys, os
import numpy as np

from pymibspline.bspline import mi
from pymibspline.LabeledMat import LabeledMat

# load clinical files

x_gen = LabeledMat.loadFile("data/Q1_training_genetic_gwas.txt", 
        dt=float, 
        verbose=False)

fixed = x_gen["19_45411941_45411941_C",:].data[0]

out = zip(x_gen.rownames, map(lambda r: mi(r, fixed) , x_gen.data))

